{
	"name": "express-boilerplate",
	"version": "1.0.0",
	"description": "Express boilerplate",
	"main": "src/App.js",
	"scripts": {
		"build": "babel ./src --out-dir ./build",
		"clean": "rm -rf ./build",
		"start": "set \"NODE_ENV=production\" && node ./build/App.js",
		"dev": "nodemon --exec npx babel-node src/app.js",
		"lint:check": "eslint src/**/*.{js,jsx}",
		"lint:fix": "eslint --fix src/**/*.{js,jsx}",
		"prettier:fix": "prettier -w \"src/**/*.js\"",
		"prettier:check": "prettier -c \"src/**/*.js\""
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/kennarddh/express-boilerplate"
	},
	"keywords": [
		"express",
		"boilerplate",
		"express-boilerplate"
	],
	"author": "kennarddh",
	"license": "ISC",
	"dependencies": {
		"body-parser": "^1.19.1",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.0.0",
		"express": "^4.17.2",
		"express-validator": "^6.14.0",
		"helmet": "^5.0.2",
		"mongoose": "^6.2.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.17.0",
		"@babel/core": "^7.17.2",
		"@babel/node": "^7.16.8",
		"@babel/plugin-transform-runtime": "^7.17.0",
		"@babel/preset-env": "^7.16.11",
		"@babel/runtime": "^7.17.2",
		"eslint": "^8.9.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-prettier": "^4.0.0",
		"nodemon": "^2.0.15",
		"prettier": "^2.5.1"
	}
}
